{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "NEXTAUTH_URL": "https://your-app-name.vercel.app"
  },
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 10
    }
  }
}
